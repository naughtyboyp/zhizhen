#include<stdio.h>
#include<math.h>
int main()
{
    char a1[] = "abcd";
    char a2[] = "abcd";
    const char* p1 = "abcd";//常量字符串不可被修改
    const char* p2 = "abcd";//为了节省空间 p1与p2中的地址相同
    if(p1 == p2)
    {
        printf("YES");
    }
    else
    {
        printf("NO");
    }
    /*if(a1 == a2)数组名表示首元素地址
    {
         printf("YES");
    }
    else
    {
        printf("NO");
    }*/
    
}
    return 0;
}
